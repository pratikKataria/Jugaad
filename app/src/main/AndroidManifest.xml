        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.Login.SignUp"></activity>
        <activity android:name=".activity.Login.Login" />
        <activity android:name=".activity.Splash.OnBoardingActivity" />
        <activity android:name=".activity.Splash.SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
